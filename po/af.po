msgstr ""

#. [unit_type]: id=Graft_half_veteran_1, race=Graft_halfling
#. [unit_type]: id=Graft_half_veteran_2, race=Graft_halfling
#. [unit_type]: id=Graft_half_veteran_3, race=Graft_halfling
